{
  "name": "@flexible-agency/website",
  "description": "Flexible Agency website.",
  "version": "1.0.0",
  "author": "Thomas Schoffelen <thomas@flexible.agency>",
  "private": true,
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "deploy": "serverless deploy",
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier"
  },
  "dependencies": {
    "@reach/router": "^1.3.4",
    "@tailwindcss/typography": "^0.4.0",
    "gatsby": "2.25.1",
    "gatsby-plugin-google-tagmanager": "^3.2.0",
    "gatsby-plugin-manifest": "2.12.1",
    "gatsby-plugin-postcss": "3.7.0",
    "gatsby-plugin-react-helmet": "3.7.0",
    "gatsby-plugin-sharp": "2.14.3",
    "gatsby-plugin-sitemap": "^3.2.0",
    "query-string": "^7.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.2.1",
    "autoprefixer": "10.2.5",
    "babel-eslint": "10.1.0",
    "cssnano": "4.1.10",
    "eslint": "7.22.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-react": "7.23.1",
    "gatsby-plugin-eslint": "2.0.8",
    "postcss": "8.2.8",
    "prettier": "2.2.1",
    "serverless": "^2.32.1",
    "serverless-cloudfront-invalidate": "^1.8.0",
    "serverless-s3-sync": "^1.17.0",
    "tailwindcss": "2.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flexible-agency/website"
  },
  "bugs": {
    "url": "https://github.com/flexible-agency/website/issues"
  }
}
